function = shape_analysis(no_of_samples)
    s = 80;
    e = 100;
    phi0 = 0;
    phi90 = pi/2;
    a = s + (s-e).*rand(1,1);
    phi = phi0 + (phi90-phi0).*rand(100,1);
    X0 = 100*rand(100,1);
    Y0 = 100*rand(100,1);
    b = a/8;
    I = zeros(2,100,no_of_samples);
    for i = 1:no_of_samples,
      I(:,:,i) =  plotellipse(a,b,X0,Y0,phi);