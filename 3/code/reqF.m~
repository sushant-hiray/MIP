function func = reqF
    func.f= @f;
    func.derX = @derX;
    func.derY = @derY;
end

function z = f(In)
    x = In(1);
    y = In(2);
    z = (1-x)^2 + 100*(y-x^2)^2;
end

function z =derX(In)
    x = In(1);
    y = In(2);
    z = -2*(1-x) + 200(y-x^2)*(-2*x)
end

function z = derY(In)
    x = In(1);
    y = In(2);
    z =200*(y-x^2);
end


    